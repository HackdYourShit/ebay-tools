jq = window.jQuery
sfx = window.sfx


jq('a.download-orders').on('click', def(evt):
    evt.preventDefault()
    jq('.status').text('download-orders started')
    jq.get('/download-orders/', def(data):
        if data == 'ok':
            jq('.status').text('download-orders finished')
            sfx.success()
    )
)
