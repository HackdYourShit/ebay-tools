from wsclient import WsClient
jq = window.jQuery


class MyClient(WsClient):
    url = '/console/messages/'
    auto_dispatch = True

    def on_open(self, evt):
        print('Websocket opened')

    def on_close(self, evt):
        print('Websocket closed')

    def on_log(self, obj):
        print(obj['value'])
        jq('<p>').text(obj['value']).appendTo('.console')


client = MyClient()
